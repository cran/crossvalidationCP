year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{crossvalidationCP}: Cross-validation in change-point regression",
         author = c(person("Florian", "Pein")),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=crossvalidationCP")
